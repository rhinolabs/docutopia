{
	"name": "@docutopia/nextjs",
	"version": "1.0.0",
	"description": "Next.js adapter for Docutopia - A modern, interactive API documentation library",
	"author": {
		"name": "Rhinolabs Agency",
		"email": "hello@rhinolabs.agency",
		"url": "https://rhinolabs.agency"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		},
		"./styles": {
			"import": "../react/dist/index.css",
			"require": "../react/dist/index.css"
		}
	},
	"files": ["dist/**/*"],
	"keywords": [
		"openapi",
		"swagger",
		"api-documentation",
		"react",
		"nextjs",
		"next.js",
		"documentation",
		"api",
		"rest"
	],
	"scripts": {
		"build": "vite build",
		"tsc": "tsc --noEmit"
	},
	"dependencies": {
		"@docutopia/react": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "^22.10.3",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@vitejs/plugin-react": "^5.0.4",
		"next": "^15.1.7",
		"typescript": "^5.7.2",
		"vite": "^7.1.11",
		"vite-plugin-dts": "^4.5.4"
	},
	"peerDependencies": {
		"next": "^14.0.0 || ^15.0.0",
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/rhinolabs/docutopia.git",
		"directory": "packages/nextjs"
	}
}
